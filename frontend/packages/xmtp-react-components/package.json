{
  "name": "@xmtp/react-components",
  "description": "React components for use with the XMTP React SDK",
  "version": "1.0.0-preview.1",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./styles.css": "./lib/index.css"
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:xmtp/xmtp-web.git",
    "directory": "packages/xmtp-react-components"
  },
  "homepage": "https://github.com/xmtp/xmtp-web",
  "bugs": {
    "url": "https://github.com/xmtp/xmtp-web/issues"
  },
  "keywords": [
    "xmtp",
    "messaging",
    "web3",
    "sdk",
    "js",
    "ts",
    "javascript",
    "typescript",
    "react",
    "reactjs",
    "react-component"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome versions",
      "last 3 firefox versions",
      "last 3 safari versions"
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean:lib": "rm -rf lib",
    "clean": "rm -rf .turbo && rm -rf node_modules && pnpm clean:lib",
    "lint": "eslint . --ignore-path ../../../archive/xmtp-web/.gitignore",
    "format:base": "prettier --ignore-path ../../../archive/xmtp-web/.gitignore",
    "format:check": "pnpm format:base -c .",
    "format": "pnpm format:base -w .",
    "generate:types": "tsup --dts-only",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@xmtp/content-type-reaction": "^1.1.2",
    "@xmtp/content-type-remote-attachment": "^1.1.2",
    "@xmtp/content-type-reply": "^1.1.3",
    "@xmtp/react-sdk": "^2.1.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-18-blockies": "^1.0.6",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@mdx-js/react": "^2.3.0",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.50.0",
    "eslint-plugin-storybook": "^0.6.14",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.30",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "peerDependencies": {
    "react": ">=16.14"
  },
  "engines": {
    "node": ">=18"
  }
}
